                        "COINBASE:ETHUSD",
                        "KUCOIN:LUNAUSDT",
                        "BINANCE:MANAUSDT",
                        "BINANCE:SHIBUSDT",
                        "BINANCE:AVAXUSDT",
                        "BINANCE:DOGEUSDT"
                      ],
                      "details": true,
                      "container_id": "tradingview_ea3eb"
