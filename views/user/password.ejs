            <li><a class="nav-link" href="/user/profile"><i class="fas fa-user-shield"></i><span>Profile</span></a></li>
            <li><a class="nav-link" href="/user/history"><i class="fas fa-receipt"></i><span>Transacton
                  History</span></a></li>
            <li><a class="nav-link" href="/user/logout"><i class="fas fa-power-off"></i><span>Logout</span></a></li>



          </ul>



        </aside>
      </div>











      <!-- Main Content -->
      <div class="main-content">
        <section class="section">


          <div class="modal-body" id="pageBody">
            <div class="card " id="depositCard">

              <div class="card-body">
                <div class="actions-toolbar py-2 mb-4">
                  <p class="text-sm text-muted mb-0">To change your password, kindly enter your old password, followed
                    by your new password twice. You are advised to chose a very strong password</p>
                </div>


                <form id="profileForm" method="POST"
                  action="https://teslastckandinvestment.com/dashboard/process/changepassword.php">

                  <div class="alert " id="profileAlert"></div>

                  <div class="form-group">
                    <label for=""> Old Password</label>
                    <input class="form-control" data-error="Kindly enter your old password" id="OldPassword"
                      placeholder="Current Password" required="" name="password" type="password">
                    <div class="help-block form-text with-errors form-control-feedback">
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label for=""> Password</label>
                        <input class="form-control" data-minlength="6" id="NewPassword" placeholder="New Password"
                          required="" name="new_password" data-val="true" autocomplete="off" data-val-length="*"
                          minlength="" maxlength="25" data-val-length-max="25" data-val-regex="*"
                          data-val-regex-pattern="" data-val-required="*" type="password">
                        <div class="help-block form-text text-muted form-control-feedback">Minimum of 6 characters</div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label for="">Confirm Password</label>
                        <input class="form-control" data-match-error="Passwords don't match"
                          placeholder="Confirm Password" required="required" type="password" name="confirm_password">
                        <div class="help-block form-text with-errors form-control-feedback">
                        </div>
                      </div>
                    </div>
                  </div>

                  <div class="form-buttons-w">
                    <input name="updateData" value="yes" type="hidden">
                    <button name="updateData" class="btn btn-primary" type="submit" id="profileBtn"> Submit</button>
                  </div>
                </form>


              </div>
            </div>
          </div>


        </section>











      </div>

      <footer class="main-footer">
        <div class="footer-left">
