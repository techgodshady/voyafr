          <div class="modal-body" id="pageBody">
            <div class="col-12 ">
              <div class="input-group mb-3">
                <h3 class="form-control" id="copy-text" onclick="copyText('copy-text')">
                  https://voyawealthmanagement.com/registration?referral=<%= refcode %></h3>

              </div>


              <div class=" table-responsive">
                <table class="table table-light align-items-center" style="min-width: 1000px;">
                  <thead>
                    <tr>
                      <th scope="col">S/N</th>
                      <th scope="col" class="sort">Name</th>
                      <th scope="col" class="sort">Email</th>
                      <th scope="col" class="sort">Referral Bonus</th>
                      <th scope="col"></th>
                    </tr>
                  </thead>
                  <tbody>







                  </tbody>
                </table>
              </div>
            </div>
          </div>

        </section>











      </div>

